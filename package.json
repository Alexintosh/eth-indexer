{
  "name": "eth-indexer",
  "version": "0.0.1",
  "description": "Blockchain indexer for Ethereum",
  "main": "dist",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src/run.js\"",
    "build": "./node_modules/.bin/babel src -s -D -d dist",
    "prod": "node dist/run.js",
    "start": "node dist/run.js",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "jest -i src",
    "test:integration": "jest -i integration"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^1.0.3",
    "bignumber.js": "^5.0.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "ethjs": "^0.3.3",
    "ethjs-abi": "^0.2.1",
    "ethjs-query": "^0.3.3",
    "ethjs-rpc": "^0.1.8",
    "ethjs-schema": "^0.2.0",
    "leveldown": "^2.1.1",
    "levelup": "^2.0.1",
    "redis": "^2.8.0",
    "rimraf": "^2.6.2",
    "web3": "^0.20.4",
    "winston": "3.0.0-rc0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "jest": "^21.2.1",
    "supertest": "^3.0.0"
  }
}
